{
    "manifest_version": 2,
    "name": "Fake F:ller",
    "description": "Fills the inputs in the current page with some fake data",
    "version": "1.0",
    "icons": {
        "16": "/images/icon-16.png",
        "48": "/images/icon-48.png",
        "128": "/images/icon-128.png"
    },
    "permissions:": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "script.js"
            ]
        }
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_icon": "/images/icon-128.png"
    }
}